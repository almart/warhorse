    ssl_prefer_server_ciphers on;
    ssl_stapling on;
    ssl_stapling_verify on;

    if ($arg_keyname = "") {
        return 302 https://{% raw %}{{ redirect_domain }}{% endraw %};
    }

    if ($is_bot) {
        return 302 https://{% raw %}{{ redirect_domain }}{% endraw %};
    }

    if ($block_ip) {
        return 302 https://{% raw %}{{ redirect_domain }}{% endraw %};
    }

